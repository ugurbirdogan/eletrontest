{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win":   "electron-packager . Ferp --overwrite --platform=win32 --arch=x64  --prune=true --out=release-builds",
    "package-linux": "electron-packager . Ferp --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.8.8",
    "electron-packager": "^15.4.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0"
  },
  "dependencies": {
    "electron-tabs": "^0.15.0",
    "favicon-getter": "^1.1.3",
    "jsonfile": "^2.4.0",
    "uuid": "^3.0.1"
  }
}
